{
    "schemaVersion" : 1,
    "id" : "${id}",
    "version" : "${version}",
    "name" : "${name}",
    "description" : "A QoL mod providing radial menu UI for Minecraft",
    "authors" : [
        "kuro-water"
    ],
    "contact" : {
        "homepage" : "https://github.com/kuro-water/KuRadialMenu",
        "sources" : "https://github.com/kuro-water/KuRadialMenu"
    },
    "license" : "MIT",
    "icon" : "assets/kuradialmenu/icon.png",
    "environment" : "client",
    "entrypoints" : {
        "client" : [
            "dev.kurowater.kuradialmenu.KuRadialMenuClient"
        ]
    },
    "mixins" : [
        "kuradialmenu.mixins.json"
    ],
    "depends" : {
        "fabricloader" : ">=0.16.0",
        "minecraft" : "${minecraft}",
        "fabric-api" : "*"
    }
}